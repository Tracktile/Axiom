{
  "name": "@tracktile/axiom",
  "version": "1.0.65",
  "description": "Dead simple, Opinionated, and Type Safe framework for building APIs with React Frontends.",
  "homepage": "https://github.com/Tracktile/Axiom",
  "license": "MIT",
  "author": "Jarred Kenny <jarred.kenny@tracktile.io (https://tracktile.io/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tracktile/Axiom.git"
  },
  "scripts": {
    "test": "CI=true vitest --globals --coverage",
    "clean": "rimraf ./*.mjs ./*.mjs.map ./*.d.ts ./*.d.mts ./*.js ./*.js.map",
    "prebuild": "yarn clean",
    "build": "tsup",
    "cli": "tsx ./src/cli"
  },
  "bin": {
    "axiom": "./cli.js"
  },
  "main": "./common.js",
  "module": "./common.mjs",
  "types": "./common.d.ts",
  "exports": {
    ".": {
      "require": "./common.js",
      "import": "./common.mjs",
      "types": "./common.d.ts"
    },
    "./client": {
      "require": "./client.js",
      "import": "./client.mjs",
      "types": "./client.d.ts"
    },
    "./server": {
      "require": "./server.js",
      "import": "./server.mjs",
      "types": "./server.d.ts"
    }
  },
  "files": [
    "*.d.ts",
    "*.d.mts",
    "*.js",
    "*.js.map",
    "*.mjs",
    "*.mjs.map"
  ],
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "@openapi-contrib/json-schema-to-openapi-schema": "^2.2.5",
    "@sinclair/typebox": "^0.31.15",
    "@tanstack/react-query": "^5.0.0-beta.20",
    "@vendia/serverless-express": "^4.10.4",
    "debug": "^4.3.4",
    "hono": "^3.5.0",
    "kebab-case": "^1.0.2",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-qs": "^3.0.0",
    "objection-find-query-builder": "^1.9.0",
    "openapi3-ts": "^4.1.2",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "ts-command-line-args": "^2.5.1",
    "ts-custom-error": "^3.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@swc/core": "^1.3.83",
    "@types/aws-lambda": "^8.10.119",
    "@types/debug": "^4.1.10",
    "@types/jest": "^29.5.3",
    "@types/koa": "^2.13.8",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-qs": "^2.0.0",
    "@types/koa__cors": "^4.0.0",
    "@types/koa__router": "^12.0.0",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.20",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@vitest/coverage-v8": "^0.34.4",
    "babel-jest": "^29.6.2",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-prune": "^0.10.3",
    "tsup": "^7.2.0",
    "tsx": "^3.14.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.4"
  },
  "peerDependencies": {}
}
